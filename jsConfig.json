{
	"compilerOptions": {
		"target": "ES6",
		"module": "commonjs",
		"allowSyntheticDefaultImports": true,
		"allowJs": true,
		"baseUrl": "./",
		"paths": {
			"~/*": ["./src/*"],
			"components/*": ["./src/components/*"],
			"config/*": ["./src/config/*"],
			"hocs/*": ["./src/hocs/*"],
			"pages/*": ["./src/pages/*"],
			"store/*": ["./src/store/*"],
			"styles/*": ["./src/styles/*"],
			"services/*": ["./src/services/*"],
			"utils/*": ["./src/utils/*"]
		}
	}
}
